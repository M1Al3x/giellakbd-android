<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragment_setup_wizard">

    <fragment
        android:id="@+id/fragment_setup_wizard"
        android:name="com.android.inputmethod.latin.setup.SetupWizardFragment"
        tools:layout="@layout/fragment_setup_wizard">
        <action
            android:id="@+id/action_fragment_setup_wizard_to_fragment_setup_complete"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"

            app:destination="@id/fragment_setup_complete" />
    </fragment>

    <fragment
        android:id="@+id/fragment_setup_complete"
        tools:layout="@layout/fragment_setup_complete"
        android:name="com.android.inputmethod.latin.setup.SetupCompleteFragment">
    </fragment>

</navigation>